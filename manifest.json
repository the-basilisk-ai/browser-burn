{
    "name": "BrowserBurn",
    "short_name": "BB",
    "description": "Spice up your new tab page with BrowserBurn, where humor meets history!",
    "version": "0.1",
    "author": "BasiliskAI",
    "minimum_chrome_version": "104",
    "manifest_version": 3,
    "host_permissions": [
        "https://api.openai.com/v1/chat/completions"
    ],
    "permissions": [
        "history",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "src/newtab/index.html"
    }
}
