        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "manifest_version": 3,
    "host_permissions": [
        "https://api.openai.com/v1/chat/completions"
    ],
    "permissions": [
        "history",
        "storage",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "src/newtab/index.html"
    }
}
